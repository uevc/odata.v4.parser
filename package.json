{
  "name": "@odata/parser",
  "version": "0.2.0",
  "type": "module",
  "description": "Zero-dependency OData v4 Parser - outputs AST for any database backend",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Bo Motlagh",
    "email": "community@ue.ventures",
    "url": "https://ue.ventures"
  },
  "contributors": [
    {
      "name": "JayStack",
      "url": "https://github.com/jaystack"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uevc/odata.v4.parser.git"
  },
  "bugs": {
    "url": "https://github.com/uevc/odata.v4.parser/issues"
  },
  "homepage": "https://github.com/uevc/odata.v4.parser#readme",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:build": "yarn build && yarn test",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist lib"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^2.0.0"
  }
}
